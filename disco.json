{
    "version": "1.0",
    "services": {
        "web": {
            "port": 8000,
            "volumes": [
                {
                    "name": "sqlite-data",
                    "destinationPath": "/sqlitedata"
                }
            ]
        },
        "hook:deploy:start:before": {
            "type": "command",
            "command": "python manage.py migrate"
        }
    }
}